{
  "name": "csfn-app",
  "version": "2.0.0",
  "private": true,
  "postinstall": "jetifier",
  "scripts": {
    "ionic:serve": "vue-cli-service serve --port 8080",
    "ionic:serve:prod": "vue-cli-service serve --mode production",
    "ionic:build": "vue-cli-service build && ionic deploy manifest && npx cap sync android",
    "build": "vue-cli-service build && ionic deploy manifest",
    "open:android": "npx cap open android",
    "lint": "vue-cli-service lint",
    "capacitor:sync:after": "node postinstall.js",
    "run:android": "vue-cli-service build && npx cap sync android && npx cap open android",
    "run:android:dev": "export NODE_ENV=development; vue-cli-service build --mode development && npx cap sync android && npx cap open android",
    "run:ios": "vue-cli-service build && npx cap sync ios && npx cap open ios",
    "resources": "node ./scripts/copy-resources/index.js",
    "resources:android": "capacitor-resources -p android -i ./resources/icon_android.png -I",
    "resources:ios": "capacitor-resources -p ios -i ./resources/icon_ios.png"
  },
